<label ($click)="switchRelation('parent')">
  <input
    type="radio"
    name="map_as--{{id}}"
    {{#if parentRelation}}checked{{/if}}
    {{#if isDisabled}}disabled{{/if}}> Parent
</label>
<label($click)="switchRelation('child')">
  <input
    type="radio"
    name="map_as--{{id}}"
    {{#if childRelation}}checked{{/if}}
    {{#if isDisabled}}disabled{{/if}}> Child
</label>
