{{^is_dashboard_or_all}}
  {{#options.isDirectlyRelated}}
    <li class="border">
        <unmap-button {source}="{parentInstance}"
        {{#if instance.snapshot}}
          {destination}="{instance.snapshot}"
        {{else}}
          {destination}="{instance}"
        {{/if}}
        class="unmap-link">
            <i class="fa fa-ban"></i>Unmap
        </unmap-button>
    </li>
  {{/options.directlyRelated}}
{{/is_dashboard_or_all}}
